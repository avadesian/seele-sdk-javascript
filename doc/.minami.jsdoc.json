{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc"]
    },
    "source": {
        "include": [
          "./src/js"
        ],
        "includePattern": ".js$",
        "excludePattern": "(node_modules/|docs)"
    },
    "plugins": [
        "plugins/markdown"
    ],
    "favicon": "./doc/_icons/s.png",
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": true,
        "useLongnameInNav": false,
        "showInheritedInNav": true,
        "favicon": "./doc/_icons/s.png"
    },
    "opts": {
        "destination": "./doc/html-jsdoc",
        "encoding": "utf8",
        "private": true,
        "recurse": true,
        "tutorials": "./doc/md-tutorials/",
        "readme": "./doc/README.md",
        "template": "./node_modules/minami"
    }
}
